{
  "dockerComposeFile": "docker-compose.yaml",
  "service": "dev",
  "workspaceFolder": "/workspace",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "charliermarsh.ruff",
        "EditorConfig.EditorConfig"
      ]
    }
  },
  "postCreateCommand": ".devcontainer/postCreateCommand.sh",
  "remoteUser": "vscode"
}
